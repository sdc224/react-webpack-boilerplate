		"@babel/plugin-transform-runtime": "7.16.4",
		"@babel/preset-env": "7.16.4",
		"@babel/preset-typescript": "7.16.0",
		"@testing-library/jest-dom": "5.15.0",
		"@testing-library/react": "12.1.2",
		"@types/history": "4.7.9",
		"@types/html-webpack-plugin": "3.2.6",
